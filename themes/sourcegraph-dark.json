	"tokenColors": [
		{
			"settings": {
				"foreground": "#D4D4D4"
				// "background": "#1E1E1E"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#2b8a3e"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#329af0"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d3f9d8"
			}
		},
		{
